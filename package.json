{
  "name": "numie-lib",
  "version": "0.1.02",
  "description": "library example",
  "main": "build/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "babel lib -d build",
    "build:watch": "babel lib -w -d build",
    "dev:rollup": "NODE_ENV=development rollup -cw",
    "prod:rollup": "NODE_ENV=production rollup -c",
    "lint": "eslint lib/** test/**",
    "lint:watch": "esw -w lib/** test/**",
    "precommit": "lint-staged",
    "review": "run-s lint test:clean test test:coverage:report",
    "test": "jest",
    "test:clean": "rimraf ./coverage",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage:report": "open ./coverage/lcov-report/index.html",
    "copy": "pwd|pbcopy && pwd"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chokidar": "^2.0.4",
    "eslint": "^5.12.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^4.0.2",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-ignore": "^1.0.4",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-multi-input": "git://github.com/eugeneross/rollup-plugin-multi-input.git",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.1.0",
    "styled-components": "^4.1.2"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**",
      "!lib/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleDirectories": [
      "node_modules",
      "./"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
